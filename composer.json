{
  "name": "mamuz/phalcon-application",
  "description": "Phalcon Application provides simple and customizable application bootstrapping",
  "type": "library",
  "homepage": "https://github.com/mamuz/phalcon-application",
  "license": "MIT",
  "keywords": [
    "Phalcon",
    "Application"
  ],
  "authors": [
    {
      "name": "Marco Muths",
      "email": "mamuz.de@gmail.com"
    }
  ],
  "support": {
    "issues": "https://github.com/mamuz/phalcon-application/issues",
    "source": "https://github.com/mamuz/phalcon-application"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true
  },
  "require": {
    "php": ">=7.0",
    "ext-phalcon": "^3.0"
  },
  "require-dev": {
    "codeception/codeception": "^2.2"
  },
  "autoload": {
    "psr-4": {
      "Phapp\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "StubProject\\": "tests/_data/StubProject/src/",
      "StubViewProject\\": "tests/_data/StubViewProject/src/"
    }
  }
}
